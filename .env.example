# Stripe Configuration (Required)
# Get these from https://dashboard.stripe.com/apikeys
STRIPE_SECRET_KEY=sk_test_your_secret_key_here
VITE_STRIPE_PUBLIC_KEY=pk_test_your_public_key_here

# Optional: Stripe Price ID for subscriptions
# STRIPE_PRICE_ID=price_your_price_id_here

# Application Configuration
NODE_ENV=development
PORT=5000

# Database (Optional - currently using in-memory storage)
# DATABASE_URL=postgresql://username:password@localhost:5432/herhealth

# Email Service (Optional - for notifications)
# RESEND_API_KEY=your_resend_api_key_here

# Video Conferencing (Optional - for Zoom integration)
# ZOOM_API_KEY=your_zoom_api_key_here
# ZOOM_API_SECRET=your_zoom_api_secret_here
